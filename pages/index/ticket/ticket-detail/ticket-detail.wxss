@import "../ticket-data/ticket-data-template.wxss";
.banner {
  width: 100%;
  margin-bottom: 20rpx;
}

.banner image {
  width: 100%;
  height: 300rpx;
}

.text-primary{
    color: #e95420;
    font-size: 25px;
    margin-top: 20rpx;
    margin-bottom: 10rpx;
    font-family: "Ubuntu",Tahoma,"Helvetica Neue",Helvetica,Arial,sans-serif;
    font-weight: 500;
    line-height: 1.1;
    width:90%;
}
.hr {
  margin-top:20rpx;
  margin-bottom:20rpx;
  border:0;
  border-top:1px solid #eeeeee;
  -webkit-box-sizing:content-box;
  -moz-box-sizing:content-box;
  box-sizing:content-box;
  height:0;
  width:90%;
}
.votes-description-content{
  margin-bottom:50rpx;
  width:90%;
  margin-top:30rpx;
}
.votes-description-content image{
  width:100%;
  height:350rpx;
}
.ticket-btn {
  width: 90%;
  background: #09bb07 !important;
  height: 80rpx;
  line-height: 80rpx;
}

.th{
  width: 50%;
}

.td{
  width: 50%;
}